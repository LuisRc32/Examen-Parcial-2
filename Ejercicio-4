#include <stdio.h>
#include <stdlib.h>
//printf("\n");
int main() {
	int V1,V2,V3,Inter;
	
	printf("Ingrese el primer numero:\n");
	scanf ("%d",&V1);
	
	printf("Ingrese el segundo numero:\n");
	scanf ("%d",&V2);
	
	printf("Ingrese el tercer numero:\n");
	scanf ("%d",&V3);
	
	Inter=V1;
	V1=V3;
	V3=V2;
	V2=Inter;
	
	
	printf("\nEstos son los numeros intercambiados:\n");
	printf("Vector 1 ahora vale: %d\n",V1);
	printf("Vector 2 ahora vale: %d\n",V2);
	printf("Vector 3 ahora vale: %d\n",V3);
	
	printf("\n\n--------FIN DEL PROGRAMA--------");
	return 0;
}
